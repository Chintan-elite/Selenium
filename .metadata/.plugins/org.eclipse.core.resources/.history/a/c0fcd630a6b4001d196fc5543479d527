package oops;

class Calc
{
	
	public void display()
	{
		System.out.println("Running display...");
	}
	
	public void add(int a, int b)
	{
		System.out.println(a+b);
	}
}

public class O005_MethodMAnipulation {
	public static void main(String[] args) {
		
		Calc c = new Calc();
		c.display();
		c.add(10,20);
		
		
	}
}
