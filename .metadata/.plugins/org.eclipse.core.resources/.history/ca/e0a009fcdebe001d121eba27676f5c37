package fileio;

import java.io.File;

public class F_FileDemo {
		
		public static void main(String[] args) {
			
			
			File file = new File("D://mywork//home.txt");
			
			
			if(file.exists())
			{
				System.out.println("File available");
			}
			else
			{
				System.out.println("File not available");
			}
			
			
		}
}
