package oops;

class NoteBook
{
	int price;
	String company;
	String color;
	
	public void display()
	{
		System.out.println(price+" "+company+" "+color);
	}
}

class Pen1
{
	public void toWrite()
	{
		System.out.println(price+" "+company+" "+color);
	}
}




public class O008_Inheitance {
	public static void main(String[] args) {
		
		NoteBook book = new NoteBook();
		book.display();
		
	}
}
