package basicPracticals;

public class B005_Armstrong {
	public static void main(String[] args) {
		
		
		int num = 154;
		int sum = 0;
		int temp = num;
		while(num!=0)
		{
			int rem = num%10;
			sum = sum + (rem*rem*rem);
			num = num/10;
			
		}
		
		System.out.println(temp);
		System.out.println(sum);
		
		
	}
}
