package collection;

import java.util.ArrayList;

public class C002_Arraylist {
	public static void main(String[] args) {
		
		
		ArrayList al = new ArrayList<>();
		al.add("Java");
		al.add(10);
		al.add(10.233);
		al.add('a');
		
		
		
		for(Object s : al)
		{
			System.out.println(s);
		}
		
		
		
		
		
		
	}
}
