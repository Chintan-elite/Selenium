
class P
{
	P()
	{
		System.out.println("P const. calling....");
	}
}

class Q
{
	Q(){
		System.out.println("Q const. calling....");
	}
}

class R extends Q
{
	R(){
		System.out.println("R const. calling...");
	}
}


public class O010_ConstructorChainnig {
	public static void main(String[] args) {
		
		
		P p = new P();
		Q q = new Q();
		R r = new R();
	}
}
