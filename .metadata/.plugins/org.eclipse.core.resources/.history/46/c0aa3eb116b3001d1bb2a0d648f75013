package oops;

class Student
{
	int id = 20;
	String name="test";
	
	public void display()
	{
		System.out.println(id+" "+name);
	}
	
}


public class O002_Second {
	public static void main(String[] args) {
		
		Student st1 = new Student();
		st1.id=50;
		st1.display();
		
		
		Student st2 = new Student();
		st2.display();
		
		
		
		
		
	}
}
