package oops;

class Emp
{
	int id;
	String name;
	String email;
	
	Emp()
	{
		System.out.println("const calling....");
	}
	
	
	public void display()
	{
		System.out.println(id+" "+name+" "+email);
	}
}

public class O003_constructor {
	public static void main(String[] args) {
		
		Emp e1 = new Emp();
		e1.display();
		
	}
}
